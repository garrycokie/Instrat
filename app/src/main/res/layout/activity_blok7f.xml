                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/page_checkbox_blok7_alasan_ragu_13_text"
                android:layout_marginBottom="10dp"/>
            <CheckBox android:id="@+id/cbQ120A14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/page_checkbox_blok7_alasan_ragu_14_text"
                android:layout_marginBottom="10dp"/>
            <CheckBox android:id="@+id/cbQ120A15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/page_checkbox_blok7_alasan_ragu_15_text"
                android:layout_marginBottom="10dp"/>
            <EditText
                android:id="@+id/txtQ120A"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="30dp"/>
        </LinearLayout>
