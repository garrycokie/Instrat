<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".activities.Blok9Activity">
    <RelativeLayout
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/lblQ128"
            android:layout_marginTop="20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/page_blok9_q128_text"
            android:layout_marginBottom="10dp"/>
        <Spinner
            android:id="@+id/spQ128A"
            android:layout_below="@+id/lblQ128"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:entries="@array/page_blok9_kategoritoko_list"
            android:layout_marginBottom="30dp"/>
    </RelativeLayout>
    <include
        layout="@layout/bottom_step_button_layout" />
</RelativeLayout>
