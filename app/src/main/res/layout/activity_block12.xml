<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.smartfren.instrat.activities.Block12Activity"
    >

   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:paddingLeft="16dp"
       android:paddingRight="16dp">
       <TextView
           android:id="@+id/txtQ134Label"
           android:layout_marginTop="20dp"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="No ID Outlet"/>

       <EditText
           android:id="@+id/txtQ134"
           android:layout_below="@id/txtQ134Label"
           android:layout_width="match_parent"
           android:layout_height="wrap_content" />

       <TextView
           android:id="@+id/txtQ135Label"
           android:layout_below="@id/txtQ134"
           android:layout_marginBottom="30dp"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:text="No ID Outlet"/>
       <Spinner
           android:id="@+id/spQ135"
           android:layout_below="@id/txtQ135Label"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content">
       </Spinner>
   </RelativeLayout>
    <include
        layout="@layout/bottom_step_button_layout" />

</RelativeLayout>
